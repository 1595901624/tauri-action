{
  "gitSiteUrl": "https://github.com/lucasfernog/tauri-action/",
  "pkgManagers": {
    "javascript": {
      "version": true,
      "publish": true
    }
  },
  "packages": {
    "action": {
      "path": "./",
      "manager": "javascript",
      "publish": [
        "git tag v${ pkgFile.versionMajor } -f",
        "git tag v${ pkgFile.versionMajor }.${ pkgFile.versionMinor } -f",
        "git tag v${ pkgFile.version }",
        "git push --tags -f"
      ]
    }
  }
}
